{
  "listen_addr":"localhost",
  "listen_port":1337,

  "grab_tty":true,

  "modules":{
    "request_handler":{
      "default_request":"static_file_req",
      "requests":{
        "/module":"client_script_req",

        "/login":"auth",
        "/logout":"auth"
      }
    },

    "static_file_req":{
      "forwards":{
        "/":"index.html"
      }
    },

    "client_script_req":{
    },

    "auth":{
      "users":{
        "GM":{
          "admin":true,
          "password":"password"
        }
      }
    }
  }
}

