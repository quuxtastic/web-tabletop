{
  "listen_addr":"localhost",
  "listen_port":1337,

  "grab_tty":true,

  "modules":{
    "request_handler":{
      "default_request":"static_file_req",
      "requests":{
        "/api/module/(\\w+)":"client_script_req:handle_module",
        "/api/modlist":"client_script_req:handle_mod_list",

        "/api/login":"auth:handle_login",
        "/api/logout":"auth:handle_logout"
      }
    },

    "static_file_req":{
      "forwards":{
        "/":"index.html"
      }
    },

    "client_script_req":{
      "client_modules":["test","load_test_1"]
    },

    "auth":{
      "users":{
        "GM":{
          "admin":true,
          "password":"password"
        }
      }
    }
  }
}

